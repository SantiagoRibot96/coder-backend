<div class="cards">
    {{#each payload}}
        <div class="card" style="width: 18rem;">
            <img src={{this.thumbnail}} class="card-img-top" alt="{{this.title}}">
            <div class="card-body">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text">{{this.description}}</p>
                <p class="card-text">${{this.price}}.-</p>
            </div>
        </div>
    {{/each}}
</div>

